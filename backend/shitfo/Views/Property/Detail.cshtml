                $(this).addClass("active");
                $(".reviews p").css("background-color", "#ddddea");
                $(".overview p").css("background-color", "white");
                $(".secondcard").show();
                $(".reviewcard").hide();
            });

            $(".dailybutton").on("click", () => {
                $(".dailybutton").addClass("active");
